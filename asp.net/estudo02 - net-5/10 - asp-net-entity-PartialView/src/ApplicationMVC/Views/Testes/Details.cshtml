@model ApplicationBusiness.CadastrosModels.Banco

@{ Layout = null; }

<div class="container">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Detalhes do Evento</h4>
            </div>
            <div class="modal-body">
                <hr />
                <dl class="dl-horizontal">
                    <dt>@Html.LabelFor(model => model.Codigo)</dt>
                    <dd>@Html.DisplayFor(model => model.Codigo)</dd>


                    <dt>@Html.LabelFor(model => model.Nome)</dt>
                    <dd>@Html.DisplayFor(model => model.Nome) </dd>

                    <dt>@Html.LabelFor(model => model.Fantasia)</dt>
                    <dd>@Html.DisplayFor(model => model.Fantasia)</dd>
                   
                </dl>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Fechar</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>